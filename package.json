    "@prisma/client": "^6.6.0",
    "@supabase/supabase-js": "^2.49.1",
    "@tsndr/cloudflare-worker-jwt": "^3.1.3",
    "@types/bun": "^1.2.4",
    "bun-types": "^1.2.4",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "hono": "^4.7.2",
    "linkedom": "^0.18.9",
    "node-html-parser": "^7.0.1",
    "prisma": "^6.6.0",
    "rxdb": "^16.9.0",
    "uuid": "^11.1.0",
    "wrangler": "^4.10.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "tsx": "^4.7.1"
  },
